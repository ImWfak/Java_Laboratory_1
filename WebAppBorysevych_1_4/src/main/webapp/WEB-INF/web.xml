<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <welcome-file-list>
        <welcome-file>BorysevychL1_WEB_Start.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>CheckLoginServlet</servlet-name>
        <servlet-class>info.webappborysevych_1_4.servlets.CheckLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckLoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowJspJstlServlet</servlet-name>
        <servlet-class>info.webappborysevych_1_4.servlets.ShowJspJstlServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowJspJstlServlet</servlet-name>
        <url-pattern>/show_jsp_jstl</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowJspServlet</servlet-name>
        <servlet-class>info.webappborysevych_1_4.servlets.ShowJspServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowJspServlet</servlet-name>
        <url-pattern>/show_jsp</url-pattern>
    </servlet-mapping>

</web-app>